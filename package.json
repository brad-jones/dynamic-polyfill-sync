{
    "name": "@brad-jones/dynamic-polyfill-sync",
    "version": "0.0.0",
    "description": "A synchronous version of dynamic-polyfill.",
    "author": "Brad Jones <brad@bjc.id.au>",
    "repository": "brad-jones/dynamic-polyfill-sync",
    "license": "MIT",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "scripts": {
        "commit": "commit",
        "commitmsg": "commitlint -e",
        "clean": "shx rm -rf ./dist",
        "build": "rollup --config rollup.config.js",
        "rebuild": "yarn clean; yarn build;",
        "test": "ts-node -P ./tests/tsconfig.json ./tests/sl-runner.ts",
        "unleash": "unleash --type=`yarn run -s conventional-recommended-bump --preset angular`"
    },
    "devDependencies": {
        "@commitlint/cli": "^4.2.1",
        "@commitlint/config-angular": "^4.2.1",
        "@commitlint/prompt-cli": "^4.2.1",
        "@types/node": "^8.0.47",
        "@types/uuid": "^3.4.3",
        "axios": "^0.17.0",
        "browserstack-local": "^1.3.0",
        "chalk": "^2.3.0",
        "conventional-recommended-bump": "^1.0.2",
        "git-last-commit": "^0.3.0",
        "husky": "^0.14.3",
        "listr": "^0.13.0",
        "local-web-server": "^2.2.4",
        "ora": "^1.3.0",
        "rollup": "^0.50.0",
        "rollup-plugin-typescript2": "^0.7.0",
        "rollup-plugin-uglify": "^2.0.1",
        "sauce-connect-launcher": "^1.2.2",
        "shx": "^0.2.2",
        "ts-node": "^3.3.0",
        "typescript": "^2.5.3",
        "unleash": "^2.0.0-beta.6",
        "uuid": "^3.1.0"
    },
    "publishConfig": {
        "access": "public"
    }
}
